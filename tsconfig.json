{
  "compilerOptions": {
    "incremental": true,
    "target": "ESNext",
    "module": "none",
    "lib": ["ESNext"],
    "sourceMap": true,
    "outDir": "./dist",
    "strict": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
